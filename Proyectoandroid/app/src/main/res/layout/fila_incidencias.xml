<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/FilaDescripcionIncidencia"
        android:layout_width="255dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="143dp"
        android:text="Descripcion"
        android:textAppearance="@android:style/TextAppearance.Medium" />


    <TextView
        android:id="@+id/FilaFechaIncidencia"
        android:layout_width="303dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/FilaDescripcionIncidencia"
        android:layout_marginStart="13dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="5dp"
        android:text="Fecha"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Small" />

    <TextView
        android:id="@+id/FilaEstadoIncidencia"
        android:layout_width="248dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/FilaFechaIncidencia"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="151dp"
        android:text="Estado" />

    <TextView
        android:id="@+id/FilaIdTipoIncidencia"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/FilaEstadoIncidencia"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="166dp"
        android:layout_marginBottom="5dp"
        android:text="Tipo Incidencia"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Small" />

    <Button
        android:id="@+id/btnVerIncidencia"
        android:layout_width="match_parent"
        android:layout_height="37dp"
        android:layout_below="@+id/FilaFechaIncidencia"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="2dp"
        android:text="Ver Incidencia"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/FilaImagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="333dp"
        android:layout_marginBottom="569dp"
        android:text="Imagen"
        android:visibility="gone" />
</RelativeLayout>